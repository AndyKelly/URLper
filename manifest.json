{
  "name": "Urlper",
  "version": "1.0",
  "description": "a quick and dirty bookmark manager",
  "permissions": [
    "bookmarks", "tabs"
  ],
  "browser_action": {
      "default_title": "Urlper.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}